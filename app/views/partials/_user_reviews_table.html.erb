<table>
  <tr>
    <th>Review Date</th>
    <th>Item Name</th>
    <th>Rating</th>
    <th>Race</th>
    <th>Comments</th>
    <th>Admin Comments</th>
  </tr>
  <% reviews.each do |review| %>
    <div class='user-review'>
      <tr>
        <td><%= review.created_at.to_date %></td>
        <td><%= review.item.brand + " " + review.item.model %></td>
        <td><%= review.rating %></td>
        <td><%= review.race.name %></td>
        <td><%= review.comments %></td>
        <td><%= review.admin_comments %></td>
      </tr>
    </div>
  <% end %>
</table>